{
	"name": "eizo-app",
	"main": "expo-router/entry",
	"version": "1.0.0",
	"scripts": {
		"start:prod": "yarn set:prod && expo start --dev-client",
		"start:local": "yarn set:local && expo start --dev-client",
		"set:prod": "sed -i '' 's|http://localhost:3000/en|https://eizo.ai/en|g' ../shared/constants/Keys.ts",
		"set:local": "sed -i '' 's|https://eizo.ai/en|http://localhost:3000/en|g' ../shared/constants/Keys.ts",
		"start": "npx expo start --dev-client",
		"postinstall": "npx patch-package",
		"android": "expo run:android",
		"ios": "expo run:ios",
		"web": " start --web",
		"test": "jest --watchAll",
		"build:ios:simulator": "yarn set:prod && eas build --profile dev-simulator --platform ios --local",
		"build:ios:devbuild": "yarn set:prod && eas build --profile development --platform ios  --clear-cache",
		"build:ios:production": "yarn set:prod && eas build --profile production --platform ios --local",
		"submit:ios": "eas submit -p ios --latest --profile production",
		"lint": "eslint .. --ext .ts,.tsx",
		"lint:fix": "eslint .. --ext .ts,.tsx --fix",
		"prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
		"update-types": "npx supabase gen types typescript --project-id \\\\\\\"rzjjwzbelffmxifrrgtv\\\\\\\" > ../nextjs/shared/types/dbSupabase.ts",
		"create-schema": "yarn supabase-to-zod --input ../nextjs/shared/types/dbSupabase.ts --output shared/types/autoGeneratedSchemas.ts",
		"show-secrets": "eas secret:list",
		"update-secrets": "eas secret:push --scope project --env-file .env --force"
	},
	"jest": {
		"preset": "jest-expo"
	},
